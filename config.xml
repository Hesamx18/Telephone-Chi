<?xml version='1.0' encoding='utf-8'?>
<widget id="com.caynix.TelephoneChi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>تلفن چی</name>
    <description>
       آسان سازی ارتباطات
    </description>
    <author email="hesam@caynix.com" href="https://caynix.com/">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://caynix.com/" />
    <allow-intent href="https://caynix.com/" />
    <allow-intent href="tel:+989156014779" />
    <allow-intent href="sms:+989156014779" />
    <allow-intent href="mailto:hesam@caynix.com" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
</widget>
